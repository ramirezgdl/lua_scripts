-- CODE HERE:

tweenService = game:GetService("TweenService")


wait()


if not _G.open then
	_G.open = true
end

if not _G.guiObject then
	_G.guiObject = {
		currentPage = nil
	}
end

tweenInfoStart = TweenInfo.new(1,Enum.EasingStyle.Back,Enum.EasingDirection.Out, 0,false,0)

tweenService:Create(Top, tweenInfoStart, {Position=UDim2.new(0.35,0,0.3,0)}):Play()

-- MENU OPEN / CLOSE
function toggleGui()
	local tweenInfo = TweenInfo.new(0.6,Enum.EasingStyle.Quart,Enum.EasingDirection.Out, 0,false,0)
	if _G.open then
		for _,window in pairs(Wrapper:GetChildren()) do
			if window:IsA("Frame") or window:IsA("ScrollingFrame") then
				window.Visible = false
			end
		end
		tweenService:Create(Wrapper, tweenInfo, {Size=UDim2.new(0,499,0,10)}):Play()
		wait(0.5)
	else
		tweenService:Create(Wrapper, tweenInfo, {Size=UDim2.new(0,499,0,278)}):Play()
		wait(0.35)
		if _G.currentPage then
			_G.currentPage.Visible = true
		end
		NavBar.Visible = true
	end
end

Close.MouseButton1Click:Connect(function()
	if _G.open then
		Close.Text = "+"
		toggleGui()
		_G.open = false
	else
		Close.Text = "-"
		toggleGui()
		_G.open = true
	end
end)

-- MENU OPEN / CLOSE
-- PAGINATION SYSTEM
for _, button in pairs(NavBar:GetChildren()) do
	if button:IsA("TextButton") then
		button.MouseButton1Click:Connect(function()
			for _, window in pairs(Wrapper:GetChildren()) do
				if window:IsA("Frame") and window.Name ~= "NavBar" then
					window.Visible = false
				end
				local selectedWindow = button.Name:split("_")[1] .. "_" .. "Window"
				if selectedWindow == "Esp_Window" then
					return game.StarterGui:SetCore("SendNotification", {
						Title="Skids Hub Message",
						Text= "We are working on esp, be patient!",
						Duration=1.5
					})
				end
				if Wrapper[selectedWindow] then
					Wrapper[selectedWindow].Visible = true
					_G.currentPage = Wrapper[selectedWindow]
					for _, button in pairs(NavBar:GetChildren()) do
						button.BackgroundColor3 = Color3.fromRGB(27,27,27)
					end
					button.BackgroundColor3 = Color3.fromRGB(65,65,65)
				else
					Donators_Window.Visible = true
				end
			end
		end)
	end
end
-- PAGINATION SYSTEM


-- SCRIPT WINDOW



-- Search Bar
searchResults = 0
SearchBar.Changed:Connect(function()
	if SearchBar.Text ~= "" then
		local formatedText = SearchBar.Text:lower()
		for _, button in pairs(Scroll:GetChildren()) do
			if button:IsA("TextButton") then
				button.Visible = false
				local textFind = string.find(button.Name:lower(), formatedText)
				if textFind then
					button.Visible = true
				end
			end
		end
		for _, button in pairs(Scroll:GetChildren()) do
			if button:IsA("TextButton") and button.Visible then
				searchResults = searchResults + 1
			end
			if searchResults >= 1 then
				Results.Visible = false
			else
				Results.Visible = true
				Results.Text = "There is no results for: " .. SearchBar.Text
			end
		end
		searchResults = 0
	else
		Results.Visible = false
		searchResults = 0
		for _, button in pairs(Scroll:GetChildren()) do
			if button:IsA("TextButton") then
				button.Visible = true
			end
		end
	end
end)
-- Search Bar


-- BUTTONS

Guns_BTN.MouseButton1Click:Connect(function()
	local Me = game.Players.LocalPlayer
	local backpack = Me:FindFirstChild("Backpack")
	if backpack then
		local tools = {}
		for _, player in pairs(game.Players:GetPlayers()) do
			if player.Name == Me.Name then continue end
			if player.TeamColor == game.Teams.Alive.TeamColor then
				if not player:FindFirstChild("Backpack") then continue end
				for _, tool in pairs(player.Backpack:GetChildren()) do
					if tool:IsA("Tool") then
						table.insert(tools, tool)
					end
				end
			end
		end
		if #tools >=1 then
			for i,tool in pairs(tools) do
				tool.Parent = backpack
			end
		else
			game.StarterGui:SetCore("SendNotification", {
				Title="Skids Hub Message",
				Text= "There is no guns found!",
				Duration=1.5
			})
		end
	end
end)


if not _G.fireInmune then
	_G.fireInmune = false
end

FireInmune_BTN.MouseButton1Click:Connect(function()
	_G.fireInmune = not _G.fireInmune
	if _G.fireInmune then
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "Fire Inmune[ON]",
			Duration=1.5
		})
	else
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "Fire Inmune[OFF]",
			Duration=1.5
		})
	end
	spawn(function()
		while true do
			if not _G.fireInmune then
				break
			end
			wait()
			local Me = game.Players.LocalPlayer
			local character = Me:FindFirstChild("Character") or Me.Character
			if character then
				for _,fire in pairs(character:GetDescendants()) do
					if fire.Name == "fire" or fire:IsA("ParticleEmitter") then
						for _,x in pairs(fire:GetChildren()) do
							if x:IsA("Script") then 
								x:Destroy()
							end
						end
					end
				end
			else
				_G.fireInmune = false
				game.StarterGui:SetCore("SendNotification", {
					Title="Skids Hub Message",
					Text= "Your character was not found!",
					Duration=1.5
				})
				break
			end
		end
	end)
end)

_G.currentTank = {tank=nil,pos=nil}
BringGasTank_BTN.MouseButton1Click:Connect(function()
	local Me = game.Players.LocalPlayer
	local character = Me:FindFirstChild("Character") or Me.Character
	local tankCounter = 0

	for _, tank in pairs(workspace.Tanks:GetChildren()) do
		if _G.currentTank.tank then
			for _, p in pairs(_G.currentTank.tank:GetChildren()) do
				if p:IsA("Part") then 
					p.CanCollide = true
					p.Position = _G.currentTank.pos
				end
			end
			_G.currentTank = {tank=nil,pos=nil}
			tankCounter = 1
			game.StarterGui:SetCore("SendNotification", {
				Title="Skids Hub Message",
				Text= "Bring Tank [OFF]",
				Duration=1.5
			})
			break
		end
		if tank:FindFirstChild("Humanoid") then
			if tank.Humanoid.Health >0 then
				tankCounter = tankCounter + 1
				_G.currentTank.tank = tank
				_G.currentTank.pos = tank:FindFirstChild("Part").Position
				for _, p in pairs(tank:GetChildren()) do
					if p:IsA("Part") then
						if character then
							if character:FindFirstChild("HumanoidRootPart") then
								p.CanCollide = false
								p.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(-2.5,0,-2)
							end
						end
					end
				end
				game.StarterGui:SetCore("SendNotification", {
					Title="Skids Hub Message",
					Text= "Bring Tank [ON]",
					Duration=1.5
				})
				break
			end
		end
	end
	if tankCounter <1 then
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "No tank alive was found!",
			Duration=1.5
		})
	end
	tankCounter = 0
end)

if not _G.ezEnabled then
	_G.ezEnabled = false
end

ezSpam_BTN.MouseButton1Click:Connect(function()
	_G.ezEnabled = not _G.ezEnabled
	if _G.ezEnabled then
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "Spam \"ez\" [ON]",
			Duration=1.5
		})
		spawn(function()
			while _G.ezEnabled do
				local A_1 = "ez"
				local A_2 = "All"
				local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
				Event:FireServer(A_1, A_2)
				wait(1)
			end
		end)
	else
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "Spam \"ez\" [OFF]",
			Duration=1.5
		})
	end
end)

Fly_BTN.MouseButton1Click:Connect(function()
	game.StarterGui:SetCore("SendNotification", {
		Title="Skids Hub Message",
		Text= "Fly Press [E]",
		Duration=1.5
	})
	repeat wait() 
	until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:findFirstChild("Humanoid") 
	local mouse = game.Players.LocalPlayer:GetMouse() 
	repeat wait() until mouse
	local plr = game.Players.LocalPlayer 
	local torso = plr.Character.Head 
	local flying = false
	local deb = true 
	local ctrl = {f = 0, b = 0, l = 0, r = 0} 
	local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
	local maxspeed = 30 
	local speed = 0 

	function Fly() 
		local bg = Instance.new("BodyGyro", torso) 
		bg.P = 9e4 
		bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
		bg.cframe = torso.CFrame 
		local bv = Instance.new("BodyVelocity", torso) 
		bv.velocity = Vector3.new(0,0.1,0) 
		bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
		repeat wait() 
			plr.Character.Humanoid.PlatformStand = true 
			if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
				speed = speed+.5+(speed/maxspeed) 
				if speed > maxspeed then 
					speed = maxspeed 
				end 
			elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
				speed = speed-1 
				if speed < 0 then 
					speed = 0 
				end 
			end 
			if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
				bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
				lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
			elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
				bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
			else 
				bv.velocity = Vector3.new(0,0.1,0) 
			end 
			bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
		until not flying 
		ctrl = {f = 0, b = 0, l = 0, r = 0} 
		lastctrl = {f = 0, b = 0, l = 0, r = 0} 
		speed = 0 
		bg:Destroy() 
		bv:Destroy() 
		plr.Character.Humanoid.PlatformStand = false 
	end 
	mouse.KeyDown:connect(function(key) 
		if key:lower() == "e" then 
			if flying then flying = false 
			else 
				flying = true 
				Fly() 
			end 
		elseif key:lower() == "w" then 
			ctrl.f = 1 
		elseif key:lower() == "s" then 
			ctrl.b = -1 
		elseif key:lower() == "a" then 
			ctrl.l = -1 
		elseif key:lower() == "d" then 
			ctrl.r = 1 
		end 
	end) 
	mouse.KeyUp:connect(function(key) 
		if key:lower() == "w" then 
			ctrl.f = 0 
		elseif key:lower() == "s" then 
			ctrl.b = 0 
		elseif key:lower() == "a" then 
			ctrl.l = 0 
		elseif key:lower() == "d" then 
			ctrl.r = 0 
		end 
	end)
	Fly()
end)

if not _G.infJumpEnabled then
	_G.infJumpEnabled = false
end

InfJump_BTN.MouseButton1Click:Connect(function()
	_G.infJumpEnabled = not _G.infJumpEnabled
	local Me = game.Players.LocalPlayer
	local mouse = Me:GetMouse()
	if _G.infJumpEnabled then
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "Infinite Jump [ON]",
			Duration=1.5
		})
		mouse.KeyDown:connect(function(key)
			if _G.infJumpEnabled then		
				if key == " " then
					game.Players.LocalPlayer.Character.Humanoid:ChangeState(3)
					wait(0.1)
				end
			else
				return
			end
		end)
	else
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "Infinite Jump [OFF]",
			Duration=1.5
		})
	end 
end)

BringTools_BTN.MouseButton1Click:Connect(function()
	local Me = game.Players.LocalPlayer
	local character = Me:FindFirstChild("Character") or Me.Character
	if character then
		local tools = {}
		for _, tool in pairs(workspace:GetChildren()) do
			if not tool:IsA("Tool") then continue end
			table.insert(tools,tool)
		end
		if #tools >=1 then
			for _, tool in pairs(tools) do
				if tool:FindFirstChildOfClass("Part") or tool:FindFirstChildOfClass("MeshPart") or tool:FindFirstChildOfClass("BasePart") then
					if character:FindFirstChild("HumanoidRootPart") then		
						tool:FindFirstChildOfClass("Part").CFrame = character.HumanoidRootPart.CFrame
					end
				end
			end
	
		else
			game.StarterGui:SetCore("SendNotification", {
				Title="Skids Hub Message",
				Text= "No tools was found!",
				Duration=1.5
			})
		end
	end
end)

NightVision_BTN.MouseButton1Click:Connect(function()
	local Me = game.Players.LocalPlayer
	local backpack = Me:FindFirstChild("Backpack")
	if backpack then
		local nightVision = nil
		for _, player in pairs(game.Players:GetPlayers()) do
			if player.Name == Me.Name then continue end
			if player.TeamColor == game.Teams.Alive.TeamColor then
				if not player:FindFirstChild("Backpack") then continue end
				for _, scriptt in pairs(player.Backpack:GetChildren()) do
					if scriptt:IsA("LocalScript") then
						nightVision = scriptt
					end
				end
			end
		end
		if nightVision then
			nightVision.Parent = backpack
		else
			game.StarterGui:SetCore("SendNotification", {
				Title="Skids Hub Message",
				Text= "No NightVision was found!",
				Duration=1.5
			})
		end
	end
end)

Sit_BTN.MouseButton1Click:Connect(function()
	local Me = game.Players.LocalPlayer
	local character = Me:FindFirstChild("Character") or Me.Character
	if character then
		local humanoid = character:FindFirstChild("Humanoid") 
		if humanoid then
			humanoid.Sit = true
		end
	end
end)

FullBright_BTN.MouseButton1Click:Connect(function()
	if not _G.FullBrightExecuted then

		_G.FullBrightEnabled = false

		_G.NormalLightingSettings = {
			Brightness = game:GetService("Lighting").Brightness,
			ClockTime = game:GetService("Lighting").ClockTime,
			FogEnd = game:GetService("Lighting").FogEnd,
			GlobalShadows = game:GetService("Lighting").GlobalShadows,
			Ambient = game:GetService("Lighting").Ambient
		}

		game:GetService("Lighting"):GetPropertyChangedSignal("Brightness"):Connect(function()
			if game:GetService("Lighting").Brightness ~= 1 and game:GetService("Lighting").Brightness ~= _G.NormalLightingSettings.Brightness then
				_G.NormalLightingSettings.Brightness = game:GetService("Lighting").Brightness
				if not _G.FullBrightEnabled then
					repeat
						wait()
					until _G.FullBrightEnabled
				end
				game:GetService("Lighting").Brightness = 1
			end
		end)

		game:GetService("Lighting"):GetPropertyChangedSignal("ClockTime"):Connect(function()
			if game:GetService("Lighting").ClockTime ~= 12 and game:GetService("Lighting").ClockTime ~= _G.NormalLightingSettings.ClockTime then
				_G.NormalLightingSettings.ClockTime = game:GetService("Lighting").ClockTime
				if not _G.FullBrightEnabled then
					repeat
						wait()
					until _G.FullBrightEnabled
				end
				game:GetService("Lighting").ClockTime = 12
			end
		end)

		game:GetService("Lighting"):GetPropertyChangedSignal("FogEnd"):Connect(function()
			if game:GetService("Lighting").FogEnd ~= 786543 and game:GetService("Lighting").FogEnd ~= _G.NormalLightingSettings.FogEnd then
				_G.NormalLightingSettings.FogEnd = game:GetService("Lighting").FogEnd
				if not _G.FullBrightEnabled then
					repeat
						wait()
					until _G.FullBrightEnabled
				end
				game:GetService("Lighting").FogEnd = 786543
			end
		end)

		game:GetService("Lighting"):GetPropertyChangedSignal("GlobalShadows"):Connect(function()
			if game:GetService("Lighting").GlobalShadows ~= false and game:GetService("Lighting").GlobalShadows ~= _G.NormalLightingSettings.GlobalShadows then
				_G.NormalLightingSettings.GlobalShadows = game:GetService("Lighting").GlobalShadows
				if not _G.FullBrightEnabled then
					repeat
						wait()
					until _G.FullBrightEnabled
				end
				game:GetService("Lighting").GlobalShadows = false
			end
		end)

		game:GetService("Lighting"):GetPropertyChangedSignal("Ambient"):Connect(function()
			if game:GetService("Lighting").Ambient ~= Color3.fromRGB(178, 178, 178) and game:GetService("Lighting").Ambient ~= _G.NormalLightingSettings.Ambient then
				_G.NormalLightingSettings.Ambient = game:GetService("Lighting").Ambient
				if not _G.FullBrightEnabled then
					repeat
						wait()
					until _G.FullBrightEnabled
				end
				game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)
			end
		end)

		game:GetService("Lighting").Brightness = 1
		game:GetService("Lighting").ClockTime = 12
		game:GetService("Lighting").FogEnd = 786543
		game:GetService("Lighting").GlobalShadows = false
		game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)

		local LatestValue = true
		spawn(function()
			repeat
				wait()
			until _G.FullBrightEnabled
			while wait() do
				if _G.FullBrightEnabled ~= LatestValue then
					if not _G.FullBrightEnabled then
						game:GetService("Lighting").Brightness = _G.NormalLightingSettings.Brightness
						game:GetService("Lighting").ClockTime = _G.NormalLightingSettings.ClockTime
						game:GetService("Lighting").FogEnd = _G.NormalLightingSettings.FogEnd
						game:GetService("Lighting").GlobalShadows = _G.NormalLightingSettings.GlobalShadows
						game:GetService("Lighting").Ambient = _G.NormalLightingSettings.Ambient
					else
						game:GetService("Lighting").Brightness = 1
						game:GetService("Lighting").ClockTime = 12
						game:GetService("Lighting").FogEnd = 786543
						game:GetService("Lighting").GlobalShadows = false
						game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)
					end
					LatestValue = not LatestValue
				end
			end
		end)
	end

	_G.FullBrightExecuted = true
	_G.FullBrightEnabled = not _G.FullBrightEnabled
	if _G.FullBrightEnabled then
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "FullBright [ON]",
			Duration=1.5
		})
	else
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "FullBright [OFF]",
			Duration=1.5
		})
	end
end)

LetalGun_BTN.MouseButton1Click:Connect(function()
	local Me = game.Players.LocalPlayer
	local Character = Me:FindFirstChild("Character") or Me.Character

	if Character then
		local gun = Character:FindFirstChildOfClass("Tool")
		if gun then
			local gun_settings = gun:FindFirstChild("Setting")
			if gun_settings then
				local gun_settings = require(gun_settings)
				gun_settings.Auto = true
				gun_settings.FireRate = 0.15
				gun_settings.BaseDamage = 100
				gun_settings.ReloadTime = 0
				gun_settings.AmmoPerClip = 100000;
				gun_settings.Clips = 100
				gun_settings.MaxClip = 100
				gun_settings.Spread = 0
				gun_settings.ReloadTime = 1
				gun_settings.CameraShakingEnabled = false
				gun_settings.ExplosiveEnabled = true
				gun_settings.Radius = 80000
				game.StarterGui:SetCore("SendNotification", {
					Title="Skids Hub Message",
					Text= "Letal Gun [ON]",
					Duration=1.5
				})
			end
		end
	end
end)

