-- Dragging script / Slider

-- Hitbox
if not _G.hitBoxData then
	_G.hitBoxData = {
		headHitBox=1,
		boxHitBox=1
	}
end

spawn(function()
	local UIS = game:GetService("UserInputService")
	
	local dragging = false
	
	Slider1.MouseButton1Down:Connect(function()
		dragging = true
	end)
	
	UIS.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local mousePosition = Vector2.new(input.Position.X,input.Position.Y)
			local sliderPosition = HeadSlider.AbsolutePosition
			local substract = (mousePosition - sliderPosition)
			local result = (substract / HeadSlider.AbsoluteSize)
			local result = math.clamp(result.X, 0, 1)
			head_hitBoxValue = math.floor((result * 100) + 1)
			HeadHitbox_Label.Text = "Head Hitbox: " .. tostring(head_hitBoxValue)
			Slider1.Position = UDim2.new(result, 0, Slider1.Position.Y.Scale,0)
		end
	end)
	
	UIS.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)
	
	-- HITBOX SAVE DATA
	SaveData_BTN.MouseButton1Click:Connect(function()
		if head_hitBoxValue then
			_G.hitBoxData.headHitBox = head_hitBoxValue
		end
	end)
	-- HITBOX SAVE DATA
	
end)

spawn(function()
	local UIS = game:GetService("UserInputService")

	local dragging = false

	Slider2.MouseButton1Down:Connect(function()
		dragging = true
	end)

	UIS.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local mousePosition = Vector2.new(input.Position.X,input.Position.Y)
			local sliderPosition = BoxHitbox.AbsolutePosition
			local substract = (mousePosition - sliderPosition)
			local result = (substract / BoxHitbox.AbsoluteSize)
			local result = math.clamp(result.X, 0, 1)
			box_hitBoxValue = math.floor((result * 100) + 1)
			BoxHitbox_Label.Text = "Box Hitbox: " .. tostring(box_hitBoxValue)
			Slider2.Position = UDim2.new(result, 0, Slider2.Position.Y.Scale,0)
		end
	end)

	UIS.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)

	-- HITBOX SAVE DATA
	SaveData_BTN.MouseButton1Click:Connect(function()
		if head_hitBoxValue then
			_G.hitBoxData.boxHitBox = box_hitBoxValue
		end
	end)
	-- HITBOX SAVE DATA

end)

spawn(function()
	game:GetService("RunService").RenderStepped:Connect(function()
		if _G.hitBoxData then
			if _G.hitBoxData.headHitBox >1 then
				for _, player in pairs(game.Players:GetPlayers()) do
					if player.Name == game.Players.LocalPlayer.Name then continue end
					local character = player:FindFirstChild("Character") or player.Character
					if character then
						if character:FindFirstChild("Head") then
							character.Head.Size = Vector3.new(_G.hitBoxData.headHitBox,_G.hitBoxData.headHitBox,_G.hitBoxData.headHitBox)
						end
					end	
				end
			else
				for _, player in pairs(game.Players:GetPlayers()) do
					if player.Name == game.Players.LocalPlayer.Name then continue end
					local character = player:FindFirstChild("Character") or player.Character
					if character then
						if character:FindFirstChild("Head") then
							character.Head.Size = Vector3.new(1,1,1)
						end
					end	
				end
			end
			if _G.hitBoxData.boxHitBox >1 then
				for _, player in pairs(game.Players:GetPlayers()) do
					if player.Name == game.Players.LocalPlayer.Name then continue end
					local character = player:FindFirstChild("Character") or player.Character
					if character then
						if character:FindFirstChild("HumanoidRootPart") then
							character.HumanoidRootPart.Size = Vector3.new(_G.hitBoxData.boxHitBox,_G.hitBoxData.boxHitBox,_G.hitBoxData.boxHitBox)
							character.HumanoidRootPart.BrickColor = BrickColor.new("Br. yellowish orange")
							character.HumanoidRootPart.Transparency = 0.6
							character.HumanoidRootPart.CanCollide = false
						end
					end	
				end
			else
				for _, player in pairs(game.Players:GetPlayers()) do
					if player.Name == game.Players.LocalPlayer.Name then continue end
					local character = player:FindFirstChild("Character") or player.Character
					if character then
						if character:FindFirstChild("HumanoidRootPart") then
							character.HumanoidRootPart.Size = Vector3.new(1,1,1)
							character.HumanoidRootPart.Transparency = 1
						end
					end	
				end
			end
		end
	end)
end)

-- Hitbox

spawn(function()
	local UIS = game:GetService("UserInputService")

	startPos = nil
	startMousePos = nil
	dragging = false


	Top.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			startPos = Top.Position
			startMousePos = input.Position
			dragging = true
		end
	end)

	UIS.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local mousePos = input.Position

			local delta = Vector2.new(input.Position.X, input.Position.Y) - Vector2.new(startMousePos.X, startMousePos.Y)
			Top.Position = UDim2.new(Top.Position.X.Scale, delta.X + startPos.X.Offset , Top.Position.Y.Scale,  delta.Y + startPos.Y.Offset )
		end
	end)

	UIS.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end
	end)

end)

-- Dragging script / Slider

-- DONATORS


spawn(function()
	local donatorTemplate = Template:Clone()
	Template:Destroy()
	while wait(1) do
		local donators_raw = game:HttpGet("https://pastebin.com/raw/KVk4Lp9F", true)
		local donator_arr = string.split(donators_raw,",")
		if #donator_arr >=1 then
			for _, label in pairs(Scroll_2:GetChildren()) do
				if label:IsA("TextLabel") then
					label:Destroy()
				end
			end
			for _, donator in pairs(donator_arr) do
				local donator_label = Template:Clone()
				donator_label.Text = donator
				donator_label.Parent = Scroll_2
			end
		end
	end
end)

-- DONATORS
