-- MAIN:

bindableDiscord = Instance.new("BindableFunction", ramirez6666)
bindableDiscord.OnInvoke = function()
	local setclip = setclipboard
	if setclip then
		setclip(getDiscordLink())
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text="Copied!",
			Duration=2
		})
	else
		local msg = Instance.new("Message", workspace)
		msg.Text = getDiscordLink()
		wait(5)
		msg:Destroy()
	end
end


function getDiscordLink()
	return game:HttpGet("urpastebinhere", true)
end

function checkVip()
	local player = game.Players.LocalPlayer
	if player:IsInGroup(10711018) then
		if player:GetRankInGroup(10711018) >=150 then
			return true
		end
	end
	return false
end


--[[if checkVip() then
	game.StarterGui:SetCore("SendNotification", {
		Title="Wanna Cry[Beta]",
		Text= game.Players.LocalPlayer.Name .. " Enjoy!!",
		Duration=2.5
	})
else
	game.StarterGui:SetCore("SendNotification", {
		Title="Skids Hub Message",
		Text= "You are not a vip user, click the button below this!",
		Callback = bindableDiscord,
		Button1="Copy Discord Link",
		Duration=6.66
	})
	return
end
]]


