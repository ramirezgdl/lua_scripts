Kill_BTN.MouseButton1Click:Connect(function()
	local Me = game.Players.LocalPlayer
	local Character = Me:FindFirstChild("Character") or Me.Character
	local backp = Me:FindFirstChild("Backpack") or Me.Backpack
	local targetBox = TargetsBox.Text
	if Character and backp then
		if targetBox ~= "" then
			if targetBox:lower() == "all" then
				for _, player in pairs(game.Players:GetPlayers()) do
					if player.Name == Me.Name then continue end
					local character = player:FindFirstChild("Character") or player.Character
					if character then
						if character:FindFirstChild("Humanoid") and character:FindFirstChild("UpperTorso") then
							local args = {
								[1] = character.Humanoid,
								[2] = character.UpperTorso,
								[3] = 1000,
								[4] = Vector3.new(666,666,666),
								[5] = 0,
								[6] = 0,
								[7] = false
							}
							for _, tool in pairs(backp:GetChildren()) do
								if not tool:IsA("Tool") then continue end
								if tool:FindFirstChild("GunScript_Server") then
									if tool["GunScript_Server"]:FindFirstChild("InflictTarget") then
										tool["GunScript_Server"]["InflictTarget"]:FireServer(unpack(args))
										break
									end
								end
							end
						end
					end
				end
			else -- "ramir,bran" 
				local separatePlayers = targetBox:split(",")
				local counter = 0
				if #separatePlayers >=1 then
					for _, separated in pairs(separatePlayers) do
						if separated == "" then continue end
						for _, player in pairs(game.Players:GetPlayers()) do
							if player.Name == Me.Name then continue end
							local character = player:FindFirstChild("Character") or player.Character
							if character then
								if character:FindFirstChild("Humanoid") and character:FindFirstChild("UpperTorso") then									
									if string.find(player.Name:lower(), separated:lower()) then
										counter = counter + 1
										local args = {
											[1] = character.Humanoid,
											[2] = character.UpperTorso,
											[3] = 1000,
											[4] = Vector3.new(666,666,666),
											[5] = 0,
											[6] = 0,
											[7] = false
										}
										for _, tool in pairs(backp:GetChildren()) do
											if not tool:IsA("Tool") then continue end
											if tool:FindFirstChild("GunScript_Server") then
												if tool["GunScript_Server"]:FindFirstChild("InflictTarget") then
													tool["GunScript_Server"]["InflictTarget"]:FireServer(unpack(args))
													break
												end
											end
										end
									end
								end
							end
						end
						if counter <1 then
							game.StarterGui:SetCore("SendNotification", {
								Title="Skids Hub Message",
								Text= "No players was found.",
								Duration=1.5
							})
						end
						counter = 0
					end
				end
			end
		else
			game.StarterGui:SetCore("SendNotification", {
				Title="Skids Hub Message",
				Text= "Something went wrong...",
				Duration=1.5
			})
		end
	else
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "Something went wrong...",
			Duration=1.5
		})
	end
end)

if not _G.bringTargets then
	_G.bringTargets = {}
end

Bring_BTN.MouseButton1Click:Connect(function()
	local Me = game.Players.LocalPlayer
	local Character = Me:FindFirstChild("Character") or Me.Character
	local backp = Me:FindFirstChild("Backpack") or Me.Backpack
	local targetBox = TargetsBox.Text
	if _G.bringTargets and #_G.bringTargets >=1 then
		_G.bringTargets = {}
		return
	end
	if Character and backp then
		if targetBox ~= "" then
			if targetBox:lower() == "all" then
				_G.bringTargets = {}
				for _, player in pairs(game.Players:GetPlayers()) do
					if player.Name == Me.Name then continue end
					local character = player:FindFirstChild("Character") or player.Character
					if character then
						if character:FindFirstChild("Humanoid") and character:FindFirstChild("HumanoidRootPart") then								
							table.insert(_G.bringTargets, player)
						end
					end
				end
			else
				_G.bringTargets = {}
				local separatePlayers = targetBox:split(",")
				local counter = 0
				if #separatePlayers >=1 then
					for _, separated in pairs(separatePlayers) do
						if separated == "" then continue end
						for _, player in pairs(game.Players:GetPlayers()) do
							if player.Name == Me.Name then continue end
							local character = player:FindFirstChild("Character") or player.Character
							if character:FindFirstChild("Humanoid") and character:FindFirstChild("HumanoidRootPart") then									
								if string.find(player.Name:lower(), separated:lower()) then
									counter = counter + 1
									table.insert(_G.bringTargets, player)
								end
							end
						end		
					end			
				end
				if counter <1 then
					game.StarterGui:SetCore("SendNotification", {
						Title="Skids Hub Message",
						Text= "No players was found.",
						Duration=1.5
					})
				end
				counter = 0
			end
		else
			_G.bringTargets = {}
			game.StarterGui:SetCore("SendNotification", {
				Title="Skids Hub Message",
				Text= "Something went wrong...",
				Duration=1.5
			})
		end
	else
		_G.bringTargets = {}
		game.StarterGui:SetCore("SendNotification", {
			Title="Skids Hub Message",
			Text= "Something went wrong...",
			Duration=1.5
		})
	end
end)

spawn(function()
	game:GetService("RunService").RenderStepped:Connect(function()
		if _G.bringTargets  and #_G.bringTargets >= 1 then
			local Me = game.Players.LocalPlayer
			local Character = Me:FindFirstChild("Character") or Me.Character
			if Character and Character:FindFirstChild("HumanoidRootPart") then
				for _, target in pairs(_G.bringTargets) do
					if target.Character then
						if target.Character:FindFirstChild("HumanoidRootPart") then
							target.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(0,2,-4)
						end
					end
				end
			end
		end
	end)
end)

